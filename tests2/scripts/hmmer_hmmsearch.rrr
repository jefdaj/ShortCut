query = load_faa "data/Mycoplasma_genitalium_single.faa"
mgen  = load_faa "data/Mycoplasma_genitalium_G37_protein_refseq.faa"

# TODO find more realistic hits here in case it breaks muscle!
hits = blastp 1e-1 query mgen
hits_ids = extract_targets hits
hits_faa = extract_seqs mgen (extract_targets hits)
hits_aln = muscle hits_faa
hits_hmm = hmmbuild hits_aln

hits2 = hmmsearch 1e-10 hits_hmm mgen
hits2_ids = extract_hmm_targets hits2

# should be []
result = hits2_ids ~ hits_ids
