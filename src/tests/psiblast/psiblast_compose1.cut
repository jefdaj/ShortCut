# psiblast functions that use compose1 function
# (this is an implementation detail, not something important to know as a user)

# setup
query   = load_faa "data/7942gene.faa"
pcc6803 = load_faa "data/6803genes.faa"
pcc7942 = load_faa "data/7942genes.faa"
db6803  = makeblastdb_prot pcc6803
db7942  = makeblastdb_prot pcc7942
pssms   = psiblast_train_db_each 1e-2 query [db7942, db6803]

# compose1-based fns to test
table1 = psiblast_pssms_db  1e-2 pssms  db7942
table2 = psiblast_pssms_all 1e-2 pssms pcc7942

# don't do this with real data unless comparable
result = concat_bht [table1, table2]
