# "base" psiblast functions that don't use any fancy helpers
# the first one trains a pssm, and the second uses it to search a db

query    = load_faa "data/7942gene.faa"
pcc7942  = load_faa "data/7942genes.faa"
db7942   = makeblastdb_prot_all [pcc7942]
pssm     = psiblast_train_db 1e-2 query db7942
hittable = psiblast_pssm_db  1e-2 pssm  db7942
result   = hittable
