# psiblast_pssms takes a list of pre-trained pssms,
# runs a psiblast search with each of them against one fasta,
# and concatenates all the hits into one table

query   = load_faa "data/7942gene.faa"
pcc6803 = load_faa "data/6803genes.faa"
pcc7942 = load_faa "data/7942genes.faa"
cyanodb = makeblastdb_prot [pcc6803, pcc7942]
pssm    = psiblast_train_db 1e-2 query  cyanodb
hitlist = psiblast_pssms    1e-2 [pssm] pcc7942
result  = hitlist
