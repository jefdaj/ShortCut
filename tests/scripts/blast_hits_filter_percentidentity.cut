small = load_faa "data/Mycoplasma_genitalium_small.faa"
mgen = load_faa "data/Mycoplasma_genitalium_G37_protein_refseq.faa"
hits = blastp 1.0e-5 small mgen
single = extract_queries (filter_percentidentity 80 hits)
mapped = extract_queries_each (filter_percentidentity_each 80 [hits])
result = length ([single] | mapped)
