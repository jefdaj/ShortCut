query    = load_faa "data/Mycoplasma_genitalium_single.faa"
mgen  = load_faa "data/Mycoplasma_genitalium_protein_refseq.faa"
maga  = load_faa "data/Mycoplasma_agalactiae_small.faa"
cyanodb  = makeblastdb_prot_all [mgen, maga]
pssm     = psiblast_train_db 1e-2 query cyanodb
hitlists = psiblast_pssm_each  1e-2 pssm [mgen, maga]
result   = hitlists
