maga10 = load_faa "data/Mycoplasma_genitalium_M2321_10genes.faa"
maga = load_faa "data/Mycoplasma_agalactiae_small.faa"
n = 1.0e-5
singleFwd = extract_queries (blastp n maga10 maga)
